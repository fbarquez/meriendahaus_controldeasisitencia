# Django settings
# Generate a new secret key for production: python -c "from django.core.management.utils import get_random_secret_key; print(get_random_secret_key())"
SECRET_KEY=change-me-in-production-use-50-chars-minimum-random

# Debug mode (set to False in production)
DEBUG=True

# Allowed hosts (comma-separated, e.g., zeit.example.com,www.zeit.example.com)
ALLOWED_HOSTS=localhost,127.0.0.1

# Timezone
TZ=Europe/Berlin
